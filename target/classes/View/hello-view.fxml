<VBox alignment="CENTER" prefHeight="1040.0" prefWidth="1669.0" spacing="20.0" style="-fx-background-color: #728C69;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="View.HelloController">
